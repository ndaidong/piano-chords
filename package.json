{
  "version": "0.0.4",
  "name": "piano-chords",
  "description": "Simple tool for determining piano chords",
  "homepage": "https://techpush.github.io/piano-chords",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/piano-chords"
  },
  "author": "@ndaidong",
  "main": "./index.js",
