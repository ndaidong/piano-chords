  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-transform-remove-strict-mode": "0.x.x",
    "babel-preset-es2015": "6.x.x",
    "bellajs": "7.x.x",
    "cheerio": "1.0.0-rc.2",
    "css-mqpacker": "6.x.x",
    "cssnano": "3.x.x",
    "eslint": "4.x.x",
    "eslint-config-ggc": "1.x.x",
    "html-minifier": "3.x.x",
    "http-server": "0.x.x",
    "minimist": "1.x.x",
    "mkdirp": "0.x.x",
    "nsp": "2.x.x",
    "postcss": "6.x.x",
    "postcss-cssnext": "3.x.x",
    "postcss-filter-plugins": "2.x.x",
    "postcss-nested": "2.x.x",
    "supertest": "3.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x",
    "testcafe": "latest",
    "testcafe-browser-provider-nightmare": "0.x.x",
    "uglify-js": "3.x.x"
